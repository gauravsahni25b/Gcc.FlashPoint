<Window x:Class="Gcc.FlashPoint.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gcc.FlashPoint"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="8*"/>
        </Grid.RowDefinitions>
        <Label Content="GCC FlashPoint" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="0"></Label>
        <TabControl Grid.Row="1">
            <TabItem TabIndex="0" Header="Input">
                <Grid>
                    <Label Content="This is Input" VerticalAlignment="Center" HorizontalAlignment="Center"></Label>
                </Grid>
            </TabItem>
            <TabItem TabIndex="1" Header="Results">
                <Grid>
                    <Label Content="This is Results" VerticalAlignment="Center" HorizontalAlignment="Center"></Label>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
